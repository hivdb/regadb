  <analysis id="phylo-ORF1" type="paup">
    <identify>
       I.1, I.2, I.3B, I.3B-8, I.3D, I.4, I.5, I.5b, I.6, I.6b, I.7, I.8, I.13, I.14,
       II.1, II.2, II.2|II.SMA, II.3, II.3R,
       II.4[0;13], II.5, II.5FR, II.5H, II.5H|II.5, II.5H|II.2, II.6,
       II.7[0;4], II.8, II.12[0;9], II.15, II.16,
       II.17, II.18, II.11/19, II.20, II.21,
       II.a, II.b[0;4], II.c, II.c|II.13, II.c|II.1, II.Erf007, II.U, O
    </identify>
    <cutoff>
      70
    </cutoff>
    <block>
      begin paup;
      log file=paup.log replace=yes;
      export format=phylip file=paup.fasta replace=yes;
      exclude gapped;
      export format=nexus file=paup.nex replace=yes;
      outgroup III.2|AY126474;
      set criterion=distance;
      dset distance=TamNei rates=gamma shape=0.333794 NegBrLen=SetAbsVal;
      constraints CONSTRE(BACKBONE)=((2,3),(4,5),(6,7,8),(10,11),(15,16),(20,21),(22,23),(25,26),(27,28),(29,30),(31,32),(35,36),(37,38),(39,40),(42,43),(45,46),(47,48),(49,50),(51,52),(54,55),(57,58),(59,60),(61,62));
      NJ constraints=CONSTRE enforce=yes;
      savetree format=nexus brlens=yes file=paup.tre replace=yes;
      bootstrap nreps=100 search=nj;
      end;
      quit;
    </block>
    <options>
      log,alignment,tree,inner,outer
    </options>
  </analysis>

