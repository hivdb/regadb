<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM
  "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">

<hibernate-reverse-engineering>
  <table-filter match-name=".*" package="net.sf.regadb.db" match-schema="public" />
  <table-filter match-name=".*" package="net.sf.regadb.db" match-schema="regadbschema" />

  <table name="patient" schema="regadbschema" class="PatientImpl">
  </table>
 
  <table name="aa_mutation" schema="regadbschema">
    <column name="nt_reference_codon" type="java.lang.String" />
    <column name="nt_mutation_codon" type="java.lang.String" />
  </table>

  <table name="aa_insertion" schema="regadbschema">
    <column name="nt_insertion_codon" type="java.lang.String" />
  </table>
</hibernate-reverse-engineering>
