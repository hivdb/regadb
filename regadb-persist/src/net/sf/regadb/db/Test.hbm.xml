<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.Test" table="test" schema="public">
    <id name="testIi" type="java.lang.Integer">
      <column name="test_ii" />
      <generator class="sequence">
        <param name="sequence">test_test_ii_seq</param>
      </generator>
    </id>
    <version name="version" type="int">
      <column name="version" not-null="true" />
    </version>
    <many-to-one name="analysis" class="net.sf.regadb.db.Analysis" fetch="select" cascade="save-update">
      <column name="analysis_ii" unique="true" />
    </many-to-one>
    <many-to-one name="testType" class="net.sf.regadb.db.TestType" fetch="select" cascade="save-update">
      <column name="test_type_ii" not-null="true" />
    </many-to-one>
    <property name="description" type="string">
      <column name="description" length="50" not-null="true" />
    </property>
  </class>
</hibernate-mapping>

