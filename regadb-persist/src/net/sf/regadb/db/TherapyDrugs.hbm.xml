<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.TherapyDrugs" table="therapy_drugs" schema="public">
    <composite-id name="id" class="net.sf.regadb.db.TherapyDrugsId">
      <key-property name="therapyIi" type="int">
        <column name="therapy_ii" />
      </key-property>
      <key-property name="drugs" type="string">
        <column name="drugs" />
      </key-property>
    </composite-id>
  </class>
</hibernate-mapping>

