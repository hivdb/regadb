<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.TherapyMotivation" table="therapy_motivation" schema="public">
    <id name="therapyMotivationIi" type="java.lang.Integer">
      <column name="therapy_motivation_ii" />
      <generator class="sequence">
        <param name="sequence">therapy_motivation_therapy_motivation_ii_seq</param>
      </generator>
    </id>
    <version name="version" type="int">
      <column name="version" not-null="true" />
    </version>
    <property name="value" type="string">
      <column name="value" length="50" not-null="true" />
    </property>
  </class>
</hibernate-mapping>

