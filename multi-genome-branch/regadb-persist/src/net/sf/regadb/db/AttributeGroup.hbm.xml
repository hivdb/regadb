<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.AttributeGroup" table="attribute_group">
    <id name="attributeGroupIi" type="java.lang.Integer">
      <column name="attribute_group_ii" />
      <generator class="native">
        <param name="sequence">attribute_group_attribute_group_ii_seq</param>
      </generator>
    </id>
    <version name="version" type="int">
      <column name="version" not-null="true" />
    </version>
    <property name="groupName" type="string">
      <column name="group_name" length="50" />
    </property>
  </class>
</hibernate-mapping>

