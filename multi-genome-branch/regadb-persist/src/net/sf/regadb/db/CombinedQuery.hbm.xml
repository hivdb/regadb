<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.CombinedQuery" table="combined_query" schema="public">
    <id name="combinedQueryIi" type="java.lang.Integer">
      <column name="combined_query_ii" />
      <generator class="sequence">
        <param name="sequence">combined_query_combined_query_ii_seq</param>
      </generator>
    </id>
    <property name="name" type="string">
      <column name="name" length="50" not-null="true" />
    </property>
  </class>
</hibernate-mapping>

