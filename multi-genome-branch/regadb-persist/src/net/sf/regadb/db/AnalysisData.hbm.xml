<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.AnalysisData" table="analysis_data" schema="public">
    <id name="analysisDataIi" type="java.lang.Integer">
      <column name="analysis_data_ii" />
      <generator class="sequence">
        <param name="sequence">analysis_data_analysis_data_ii_seq</param>
      </generator>
    </id>
    <many-to-one name="analysis" class="net.sf.regadb.db.Analysis" fetch="select" cascade="save-update">
      <column name="analysis_ii" not-null="true" />
    </many-to-one>
    <property name="name" type="string">
      <column name="name" length="50" />
    </property>
    <property name="data" type="binary">
      <column name="data" />
    </property>
    <property name="mimetype" type="string">
      <column name="mimetype" length="50" not-null="true" />
    </property>
  </class>
</hibernate-mapping>

