     <DRUG>
         <NAME>DDI</NAME>
         <!-- ***** the following 3 rules for ddi are an 
 interpretation of the following anrs rule ***** -->
         <!-- ***** at least a score of +2 among: M41L + T69D + L74V 
 + T215FY + K219QE - K70R - M184VI ***** -->
         <RULE>
             <CONDITION>
                 EXCLUDE 70R AND EXCLUDE 184VI AND
                 SELECT ATLEAST 2 FROM (41L,69D,74V,215FY,219QE)
