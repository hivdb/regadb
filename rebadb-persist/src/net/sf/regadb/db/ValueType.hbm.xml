<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="net.sf.regadb.db.ValueType" table="value_type" schema="public">
    <id name="valueTypeIi" type="java.lang.Integer">
      <column name="value_type_ii" />
      <generator class="sequence">
        <param name="sequence">value_type_value_type_ii_seq</param>
      </generator>
    </id>
    <version name="version" type="java.lang.Integer">
      <column name="version" />
    </version>
    <property name="description" type="string">
      <column name="description" length="50" not-null="true" />
    </property>
    <property name="min" type="java.lang.Double">
      <column name="min" precision="8" scale="0" />
    </property>
    <property name="max" type="java.lang.Double">
      <column name="max" precision="8" scale="0" />
    </property>
    <property name="multiple" type="java.lang.Boolean">
      <column name="multiple" />
    </property>
  </class>
</hibernate-mapping>

